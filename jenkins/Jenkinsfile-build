library('private-pipeline-library')
library('jenkins-shared')

mavenSnapshotPipeline(performSonarAnalysis: true, usePublicSettingsXmlFile: true, deployBranch: 'testing-jenkins',
    onSuccess: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nxrm-notifications')
    },
    onFailure: { build, env ->
      notifyChat(env: env, currentBuild: build, room: 'nxrm-notifications')
    }
)
